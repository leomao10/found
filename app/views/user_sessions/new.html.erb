<% title "Login" %>

<% form_for @user_session do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :username %><br />
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %>
    <%= link_to "forgot password", new_password_reset_path %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>


